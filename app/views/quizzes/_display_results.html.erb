<% results.each do |r| %>
    <div class="student-result-block">
        <div class="quiz-block group">
            <p><%= r.user.full_name %> | Grade: <%= r.grade %></p>
        </div>
        <div class="quiz-block group">
            <% @note = r.user.notes.find {|n| n.quiz == @quiz }%>
            <% if @note %>
                <p><%= @note.content %></p>
            <% end %>
        </div>

    </div>
<% end %>